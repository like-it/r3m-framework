{
  "package" : {
    "r3m_io/event" : {
      "composer" : "composer require {{$this.#key}}",
      "route" : [
        "{{config('project.dir.vendor')}}{{$this.#key}}/Data/Route.json}}"
      ],
      "installation" : "{{config('project.dir.data')}}{{config('dictionary.package')}}{{config('ds')}}{{$this.#key}}{{config('ds')}}{{config('dictionary.installation')}}{{config('extension.json')}}",
      "copy" : [
        {
          "from" : "{{config('project.dir.vendor')}}{{$this.#parent.#key}}/src/",
          "to" : "{{config('project.dir.root')}}Package/{{$this.#parent.#key}}/",
          "recursive": true
        }
      ],
      "command" : [
        "{{binary()}} {{$this.#key}} setup"
      ],
      "update" : {
        "command" : [
          "{{binary()}} {{$this.#parentNode.#key}} update"
        ]
      }
    },
    "r3m_io/node" : {
